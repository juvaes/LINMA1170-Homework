close all; clc;

alpha = -100;
beta = 100;
numberRoots = 100;
timeExecution = 0*numberRoots;

for i = 1:length(numberRoots)
    %coefficient = randi(10,1,numberRoots(i));
    coefficient = randi(10,1,numberRoots(i));
    tic;
    [V,R] = AlgEuclideJulien(coefficient,alpha,beta);
    r = imag(roots(coefficient));
    timeExecution(i) = toc;
end

c = 0;
for i = 1:length(r)
    if r(i)==0
        c=c+1;
    end
end
S1  = V
S2 = c
 

